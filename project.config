[access]
	inheritFrom = All-Projects
[submit]
	action = inherit
[access "refs/for/*"]
	label-Code-Review = -2..+2 group Raphael maintainers
	create = group Raphael maintainers
	push = +force group Raphael maintainers
	pushMerge = group Raphael maintainers
	label-Verified = -1..+1 group Raphael maintainers
	submit = group Raphael maintainers
[access "refs/for/refs/heads/*"]
	pushMerge = group Raphael maintainers
